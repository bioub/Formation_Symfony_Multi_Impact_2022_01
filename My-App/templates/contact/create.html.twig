{% form_theme contactForm 'bootstrap_5_layout.html.twig' %}
{% extends 'base.html.twig' %}

{% block title %}Hello ContactController!{% endblock %}

{% block body %}
    {{ form_start(contactForm, {attr: {novalidate: 'novalidate'}}) }}
    {{ form_row(contactForm.name) }}
    {{ form_row(contactForm.email) }}
    {{ form_row(contactForm.societe) }}
    {{ form_row(contactForm.groupes) }}
    <button>OK</button>
    {{ form_end(contactForm) }}
{% endblock %}


{% block javascripts %}
    {{ parent() }}
    {{ encore_entry_script_tags('contact-form') }}
{% endblock %}